{
  "name": "@alexdiliberto/eslint-config",
  "version": "3.1.0",
  "description": "Alex DiLiberto's preferred ESLint rules",
  "keywords": [
    "eslint",
    "eslint-config",
    "eslintconfig"
  ],
  "license": "MIT",
  "author": "Alex DiLiberto <npm@alexdiliberto.com>",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexdiliberto/eslint-config.git"
  },
  "bugs": {
    "url": "https://github.com/alexdiliberto/eslint-config/issues"
  },
  "scripts": {
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 1",
    "commit": "git-cz",
    "deploy": "np",
    "test": "yarn test:unit:spec",
    "test:unit": "tape test/*.js",
    "test:unit:spec": "tape test/*.js | tap-spec",
    "version": "yarn changelog && git add CHANGELOG.md"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-angular": "^8.1.0",
    "commitizen": "^4.0.3",
    "conventional-changelog-cli": "^2.0.11",
    "cz-conventional-changelog": "^3.0.2",
    "eslint": "^6.4.0",
    "husky": "^3.0.2",
    "np": "^5.0.1",
    "tap-spec": "^5.0.0",
    "tape": "^4.9.1"
  },
  "peerDependencies": {
    "eslint": ">= 5.11.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "engines": {
    "node": ">= 10"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e"
    }
  }
}
