{
  "name": "@alexdiliberto/eslint-config",
  "version": "4.2.0",
  "description": "Alex DiLiberto's preferred ESLint rules",
  "keywords": [
    "eslint",
    "eslint-config",
    "eslintconfig"
  ],
  "license": "MIT",
  "author": "Alex DiLiberto <npm@alexdiliberto.com>",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/alexdiliberto/eslint-config.git"
  },
  "bugs": {
    "url": "https://github.com/alexdiliberto/eslint-config/issues"
  },
  "scripts": {
    "changelog": "git-chglog -o CHANGELOG.md",
    "commit": "git-cz",
    "release": "release-it",
    "test": "yarn test:unit:spec",
    "test:unit": "tape test/*.js",
    "test:unit:spec": "tape test/*.js | tap-spec",
    "version": "git-chglog --next-tag v$npm_package_version -o CHANGELOG.md && git add CHANGELOG.md"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-angular": "^12.1.4",
    "commitizen": "^4.0.3",
    "cz-conventional-changelog": "^3.0.2",
    "eslint": "^7.0.0",
    "husky": "^4.2.3",
    "release-it": "^14.0.2",
    "tap-spec": "^5.0.0",
    "tape": "^5.0.0"
  },
  "peerDependencies": {
    "eslint": ">= 5.11.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "engines": {
    "node": ">= 10"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e"
    }
  },
  "volta": {
    "node": "12.22.1",
    "yarn": "1.22.10"
  }
}
