{
  "name": "@alexdiliberto/eslint-config",
  "version": "7.0.2",
  "description": "Alex DiLiberto's preferred ESLint rules",
  "keywords": [
    "eslint",
    "eslint-config",
    "eslintconfig"
  ],
  "homepage": "https://github.com/alexdiliberto/eslint-config#readme",
  "bugs": {
    "url": "https://github.com/alexdiliberto/eslint-config/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexdiliberto/eslint-config.git"
  },
  "license": "MIT",
  "author": "Alex DiLiberto <npm@alexdiliberto.com>",
  "sideEffects": false,
  "type": "commonjs",
  "exports": {
    ".": "./flat.js",
    "./flat": "./flat.js"
  },
  "main": "flat.js",
  "files": [
    "flat.js",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "changelog": "git-chglog -o CHANGELOG.md",
    "clean": "rm -rf node_modules",
    "commit": "git-cz",
    "preinstall": "npx only-allow pnpm",
    "lint": "eslint . --ext .js --no-error-on-unmatched-pattern",
    "prepare": "husky",
    "release": "release-it",
    "test": "pnpm test:unit:spec",
    "test:fix": "node -e \"console.log('run full test suite; fix assertions are included')\"",
    "test:unit": "tape test/*.js",
    "test:unit:spec": "tape test/*.js | tap-spec",
    "version": "git-chglog --next-tag v$npm_package_version -o CHANGELOG.md && git add CHANGELOG.md"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^20.3.1",
    "@commitlint/config-conventional": "^20.3.1",
    "@stylistic/eslint-plugin": "^5.7.0",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.39.2",
    "husky": "^9.1.7",
    "release-it": "^19.2.4",
    "tap-spec": "^5.0.0",
    "tape": "^5.9.0"
  },
  "peerDependencies": {
    "@stylistic/eslint-plugin": "^5.0.0",
    "eslint": "^9.0.0"
  },
  "packageManager": "pnpm@10.28.0",
  "engines": {
    "node": ">=20.9.0 || ^22 || ^24"
  },
  "volta": {
    "node": "24.11.0",
    "pnpm": "10.28.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
