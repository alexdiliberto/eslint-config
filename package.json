{
  "name": "@alexdiliberto/eslint-config",
  "version": "6.0.1",
  "description": "Alex DiLiberto's preferred ESLint rules",
  "keywords": [
    "eslint",
    "eslint-config",
    "eslintconfig"
  ],
  "license": "MIT",
  "author": "Alex DiLiberto <npm@alexdiliberto.com>",
  "main": "index.js",
  "type": "commonjs",
  "homepage": "https://github.com/alexdiliberto/eslint-config#readme",
  "files": [
    "index.js",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/alexdiliberto/eslint-config.git"
  },
  "bugs": {
    "url": "https://github.com/alexdiliberto/eslint-config/issues"
  },
  "scripts": {
    "changelog": "git-chglog -o CHANGELOG.md",
    "clean": "rm -rf node_modules",
    "commit": "git-cz",
    "release": "release-it",
    "prepare": "husky install",
    "lint": "eslint . --ext .js --no-error-on-unmatched-pattern",
    "test": "pnpm test:unit:spec",
    "test:fix": "node -e \"console.log('run full test suite; fix assertions are included')\"",
    "test:unit": "tape test/*.js",
    "test:unit:spec": "tape test/*.js | tap-spec",
    "version": "git-chglog --next-tag v$npm_package_version -o CHANGELOG.md && git add CHANGELOG.md"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.3.0",
    "@commitlint/config-angular": "^16.3.0",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.57.0",
    "husky": "^7.0.4",
    "release-it": "^14.14.3",
    "tap-spec": "^5.0.0",
    "tape": "^5.7.5"
  },
  "peerDependencies": {
    "eslint": ">= 8.57.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "engines": {
    "node": ">= 18.8.0"
  },
  "volta": {
    "node": "20.19.5"
  },
  "packageManager": "pnpm@10.20.0+sha512.cf9998222162dd85864d0a8102e7892e7ba4ceadebbf5a31f9c2fce48dfce317a9c53b9f6464d1ef9042cba2e02ae02a9f7c143a2b438cd93c91840f0192b9dd"
}
